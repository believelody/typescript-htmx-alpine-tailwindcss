<nav class="grid grid-cols-4 list-none" hx-boot="true">
  {{#each brands as |brand|}}
    <li>
      <a hx-indicator="#loader, #content" href="/brands/{{ brand }}" hx-push-url="true" hx-target="#content"
          hx-swap="innerHTML" class="text-lg text-gray-600 cursor-pointer hover:text-gray-800">{{ brand }}</a>
    </li>
  {{/each}}
</nav>